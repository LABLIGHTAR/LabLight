.video-container {
    align-self: center;
    padding: 10px;
    background-color: rgb(30, 30, 30);
    border-radius: 10px;
}

.protocol-video-player {
    max-height: 400px;
    margin-bottom: 10px;
    border-radius: 6px;
}

.video-controls-container {
    flex-direction: row;
    align-items: center;
}

.video-control-button {
    width: 32px;
    height: 32px;
    margin-right: 10px;
}

.video-progress-bar {
    flex-grow: 1;
} 