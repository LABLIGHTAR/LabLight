{
    "$id": "1",
    "version": 9,
    "title": "Gel Extraction Kit",
    "pdfPath": "Procedure/Gel Extraction Kit/Gel Extraction.pdf",
    "globalArElements": [],
    "steps": [
        {
            "$id": "8",
            "isCritical": false,
            "arElements": [
                {
                    "$id": "9",
                    "layout": {
                        "$id": "10",
                        "layoutType": "Vertical",
                        "contentItems": [
                            {
                                "$id": "11",
                                "textType": "Header",
                                "text": "Safety and Preparation",
                                "fontsize": 7,
                                "contentType": "Text"
                            }
                        ],
                        "contentType": "Layout"
                    },
                    "arDefinitionType": "Container",
                    "target": "",
                    "Count": 1,
                    "IsReadOnly": false
                }
            ],
            "checklist": [
                {
                    "$id": "2",
                    "Text": "Wear a suitable lab coat, disposable gloves, and protective goggles.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "3",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                },
                {
                    "$id": "4",
                    "Text": "Consult the appropriate material safety data sheets (MSDSs) for chemicals.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "5",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                },
                {
                    "$id": "6",
                    "Text": "Prepare necessary materials: Ethanol (96-100%), microcentrifuge, 1.5 or 2 ml microcentrifuge tubes, 3 M sodium acetate (pH 5.0), optional distilled water or TE buffer (10 mM Tris\u00b7Cl, 1 mM EDTA, pH 8.0).",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "7",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                }
            ],
            "SignedOff": false
        },
        {
            "$id": "22",
            "isCritical": false,
            "arElements": [
                {
                    "$id": "23",
                    "layout": {
                        "$id": "24",
                        "layoutType": "Vertical",
                        "contentItems": [
                            {
                                "$id": "25",
                                "textType": "Header",
                                "text": "Pre-Procedure Notes",
                                "fontsize": 7,
                                "contentType": "Text"
                            }
                        ],
                        "contentType": "Layout"
                    },
                    "arDefinitionType": "Container",
                    "target": "",
                    "Count": 1,
                    "IsReadOnly": false
                }
            ],
            "checklist": [
                {
                    "$id": "12",
                    "Text": "Ensure the protocol is suitable for the purification of up to 10 \u00b5g DNA (70 bp to 10 kb).",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "13",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                },
                {
                    "$id": "14",
                    "Text": "Ensure the yellow color of Buffer QG indicates a pH \u22647.5.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "15",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                },
                {
                    "$id": "16",
                    "Text": "Add ethanol (96-100%) to Buffer PE before use (refer to bottle label for volume).",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "17",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                },
                {
                    "$id": "18",
                    "Text": "Prepare isopropanol (100%) and have a heating block or water bath at 50\u00b0C available.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "19",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                },
                {
                    "$id": "20",
                    "Text": "Set the centrifuge to 17,900 x g (13,000 rpm) for all centrifugation steps.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "21",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                }
            ],
            "SignedOff": false
        },
        {
            "$id": "32",
            "isCritical": false,
            "arElements": [
                {
                    "$id": "33",
                    "layout": {
                        "$id": "34",
                        "layoutType": "Vertical",
                        "contentItems": [
                            {
                                "$id": "35",
                                "textType": "Header",
                                "text": "Excise DNA Fragment",
                                "fontsize": 7,
                                "contentType": "Text"
                            }
                        ],
                        "contentType": "Layout"
                    },
                    "arDefinitionType": "Container",
                    "target": "",
                    "Count": 1,
                    "IsReadOnly": false
                }
            ],
            "checklist": [
                {
                    "$id": "26",
                    "Text": "Excise the DNA fragment from the agarose gel with a clean, sharp scalpel.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "27",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                },
                {
                    "$id": "28",
                    "Text": "Weigh the gel slice in a colorless tube.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "29",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                },
                {
                    "$id": "30",
                    "Text": "Add 3 volumes Buffer QG to 1 volume gel (100 mg gel \u2248 100 \u00b5l). For >2% agarose gels, add 6 volumes Buffer QG.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "31",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                }
            ],
            "SignedOff": false
        },
        {
            "$id": "42",
            "isCritical": false,
            "arElements": [
                {
                    "$id": "43",
                    "layout": {
                        "$id": "44",
                        "layoutType": "Vertical",
                        "contentItems": [
                            {
                                "$id": "45",
                                "textType": "Header",
                                "text": "Dissolve Gel Slice",
                                "fontsize": 7,
                                "contentType": "Text"
                            }
                        ],
                        "contentType": "Layout"
                    },
                    "arDefinitionType": "Container",
                    "target": "",
                    "Count": 1,
                    "IsReadOnly": false
                }
            ],
            "checklist": [
                {
                    "$id": "36",
                    "Text": "Incubate at 50\u00b0C for 10 minutes (or until the gel slice has completely dissolved).",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "37",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                },
                {
                    "$id": "38",
                    "Text": "Vortex the tube every 2-3 minutes to help dissolve the gel.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "39",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                },
                {
                    "$id": "40",
                    "Text": "Ensure the color of the mixture is yellow. If the color is orange or violet, add 10 \u00b5l 3 M sodium acetate (pH 5.0) and mix until the mixture turns yellow.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "41",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                }
            ],
            "SignedOff": false
        },
        {
            "$id": "54",
            "isCritical": false,
            "arElements": [
                {
                    "$id": "55",
                    "layout": {
                        "$id": "56",
                        "layoutType": "Vertical",
                        "contentItems": [
                            {
                                "$id": "57",
                                "textType": "Header",
                                "text": "Binding DNA to QIAquick Column",
                                "fontsize": 7,
                                "contentType": "Text"
                            }
                        ],
                        "contentType": "Layout"
                    },
                    "arDefinitionType": "Container",
                    "target": "",
                    "Count": 1,
                    "IsReadOnly": false
                }
            ],
            "checklist": [
                {
                    "$id": "46",
                    "Text": "Add 1 gel volume isopropanol to the sample and mix.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "47",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                },
                {
                    "$id": "48",
                    "Text": "Place a QIAquick spin column in a provided 2 ml collection tube or into a vacuum manifold.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "49",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                },
                {
                    "$id": "50",
                    "Text": "To bind DNA, apply the sample to the QIAquick column and centrifuge for 1 minute or apply vacuum.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "51",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                },
                {
                    "$id": "52",
                    "Text": "Discard flow-through and place the QIAquick column back into the same tube. For sample volumes >800 \u00b5l, load and spin/apply vacuum again.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "53",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                }
            ],
            "SignedOff": false
        },
        {
            "$id": "62",
            "isCritical": false,
            "arElements": [
                {
                    "$id": "63",
                    "layout": {
                        "$id": "64",
                        "layoutType": "Vertical",
                        "contentItems": [
                            {
                                "$id": "65",
                                "textType": "Header",
                                "text": "Optional Cleansing Step for Sensitive Applications",
                                "fontsize": 7,
                                "contentType": "Text"
                            }
                        ],
                        "contentType": "Layout"
                    },
                    "arDefinitionType": "Container",
                    "target": "",
                    "Count": 1,
                    "IsReadOnly": false
                }
            ],
            "checklist": [
                {
                    "$id": "58",
                    "Text": "Add 500 \u00b5l Buffer QG to the QIAquick column and centrifuge for 1 minute or apply vacuum.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "59",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                },
                {
                    "$id": "60",
                    "Text": "Discard flow-through and place the QIAquick column back into the same tube.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "61",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                }
            ],
            "SignedOff": false
        },
        {
            "$id": "72",
            "isCritical": false,
            "arElements": [
                {
                    "$id": "73",
                    "layout": {
                        "$id": "74",
                        "layoutType": "Vertical",
                        "contentItems": [
                            {
                                "$id": "75",
                                "textType": "Header",
                                "text": "Wash QIAquick Column",
                                "fontsize": 7,
                                "contentType": "Text"
                            }
                        ],
                        "contentType": "Layout"
                    },
                    "arDefinitionType": "Container",
                    "target": "",
                    "Count": 1,
                    "IsReadOnly": false
                }
            ],
            "checklist": [
                {
                    "$id": "66",
                    "Text": "Add 750 \u00b5l Buffer PE to QIAquick column and centrifuge for 1 minute or apply vacuum.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "67",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                },
                {
                    "$id": "68",
                    "Text": "Discard flow-through and place the QIAquick column back into the same tube.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "69",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                },
                {
                    "$id": "70",
                    "Text": "If DNA will be used for salt-sensitive applications, let the column stand for 2-5 minutes after the addition of Buffer PE.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "71",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                }
            ],
            "SignedOff": false
        },
        {
            "$id": "80",
            "isCritical": false,
            "arElements": [
                {
                    "$id": "81",
                    "layout": {
                        "$id": "82",
                        "layoutType": "Vertical",
                        "contentItems": [
                            {
                                "$id": "83",
                                "textType": "Header",
                                "text": "Remove Residual Wash Buffer",
                                "fontsize": 7,
                                "contentType": "Text"
                            }
                        ],
                        "contentType": "Layout"
                    },
                    "arDefinitionType": "Container",
                    "target": "",
                    "Count": 1,
                    "IsReadOnly": false
                }
            ],
            "checklist": [
                {
                    "$id": "76",
                    "Text": "Centrifuge the QIAquick column in the provided 2 ml collection tube for 1 minute to remove residual wash buffer.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "77",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                },
                {
                    "$id": "78",
                    "Text": "Spin once more to remove residual ethanol.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "79",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                }
            ],
            "SignedOff": false
        },
        {
            "$id": "92",
            "isCritical": false,
            "arElements": [
                {
                    "$id": "93",
                    "layout": {
                        "$id": "94",
                        "layoutType": "Vertical",
                        "contentItems": [
                            {
                                "$id": "95",
                                "textType": "Header",
                                "text": "Elute DNA",
                                "fontsize": 7,
                                "contentType": "Text"
                            }
                        ],
                        "contentType": "Layout"
                    },
                    "arDefinitionType": "Container",
                    "target": "",
                    "Count": 1,
                    "IsReadOnly": false
                }
            ],
            "checklist": [
                {
                    "$id": "84",
                    "Text": "Place QIAquick column into a clean 1.5 ml microcentrifuge tube.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "85",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                },
                {
                    "$id": "86",
                    "Text": "Add 50 \u00b5l Buffer EB (10 mM Tris-Cl, pH 8.5) or water to the center of the QIAquick membrane and centrifuge for 1 minute.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "87",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                },
                {
                    "$id": "88",
                    "Text": "For increased DNA concentration, add 30 \u00b5l Buffer EB to the center of the QIAquick membrane, let the column stand for 1 minute, and then centrifuge for 1 minute.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "89",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                },
                {
                    "$id": "90",
                    "Text": "For higher yields, increase the incubation time to up to 4 minutes.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "91",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                }
            ],
            "SignedOff": false
        },
        {
            "$id": "100",
            "isCritical": false,
            "arElements": [
                {
                    "$id": "101",
                    "layout": {
                        "$id": "102",
                        "layoutType": "Vertical",
                        "contentItems": [
                            {
                                "$id": "103",
                                "textType": "Header",
                                "text": "Gel Analysis of Purified DNA",
                                "fontsize": 7,
                                "contentType": "Text"
                            }
                        ],
                        "contentType": "Layout"
                    },
                    "arDefinitionType": "Container",
                    "target": "",
                    "Count": 1,
                    "IsReadOnly": false
                }
            ],
            "checklist": [
                {
                    "$id": "96",
                    "Text": "Add 1 volume of Loading Dye to 5 volumes of purified DNA.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "97",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                },
                {
                    "$id": "98",
                    "Text": "Mix the solution by pipetting up and down before loading the gel.",
                    "activateTimer": false,
                    "hours": 0,
                    "minutes": 0,
                    "seconds": 0,
                    "CompletionTime": "0001-01-01T00:00:00",
                    "IsChecked": {
                        "$id": "99",
                        "Value": false,
                        "HasValue": true,
                        "value": false
                    },
                    "arElements": [],
                    "operations": []
                }
            ],
            "SignedOff": false
        }
    ],
    "mediaBasePath": "Procedure/QIAquick_Gel_Extraction_Kit__QIAquick_PCR__Gel_Cleanup_Kit.pdf"
