      "Checklist": [
        {
          "Text": "Locate the reservoir containing distilled water.",
          "ArAction": {
            "actionType": "Highlight",
            "arObjectID": "reservoir_01",
            "properties": {
              "subIDs": ["1"],
              "colorHex": "#FFD700",
              "colorName": "Gold"
            }
          },
          "ContentItems": [
